<Window x:Class="Grafik.DiagramShowHelper"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Grafik"
        mc:Ignorable="d"
        Title="DiagramShowHelper" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock x:Name="textBlock"></TextBlock>
            <TextBlock x:Name="FULLWORKDAY"></TextBlock>
            <TextBlock x:Name="FULLWORKNIGHT"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
